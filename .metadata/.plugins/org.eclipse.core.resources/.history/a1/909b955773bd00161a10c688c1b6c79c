package ArvoreBinaria;

public class No {
	
	public No esq;
	public No dir;
	public char valor;
	
	public No(){
		this.esq = null;
		this.dir = null;
		this.valor = 0;
	}
	
	public No(char valor){
		this.valor = valor;
	}
	
	public void setEsq(No no){
		this.esq = no;
	}
	
	public void setDir(No no){
		this.dir = no;
	}
	
	public char getValor(){
		return this.valor;
	}
	
	public No getEsq(){
		return this.esq;
	}
	
	public No getDir(){
		return this.dir;
	}
}
